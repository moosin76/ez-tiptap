{
  "name": "@ezvue/ez-tiptap-editor",
  "version": "0.1.1",
  "description": "vue vuetify tiptap Rich Text Editor",
  "private": false,
  "scripts": {
    "start": "nodemon app.js",
    "devBuild": "vue-cli-service build dev/serve.js",
    "dev": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "main": "dist/ez-tiptap.ssr.js",
  "module": "dist/ez-tiptap.esm.js",
  "browser": "dist/ez-tiptap.esm.js",
  "unpkg": "dist/ez-tiptap.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@mdi/font": "5.9.55",
    "@tiptap/extension-code-block-lowlight": "^2.0.0-beta.25",
    "@tiptap/extension-document": "^2.0.0-beta.12",
    "@tiptap/extension-font-family": "^2.0.0-beta.12",
    "@tiptap/extension-highlight": "^2.0.0-beta.13",
    "@tiptap/extension-image": "^2.0.0-beta.13",
    "@tiptap/extension-link": "^2.0.0-beta.17",
    "@tiptap/extension-paragraph": "^2.0.0-beta.14",
