    "@vue/cli-service": "^4.5.10",
    "autoprefixer": "^10.2.6",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.3.2",
    "postcss-import": "^14.0.2",
    "rollup": "^2.36.1",
